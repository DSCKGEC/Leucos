        <circle cx="10" cy="10" r="90" fill="#99FEFF" />
    </svg>
    <img id="groupchat" src="/static/img/groupchat.svg" alt="">
    <div class="text">Enjoy group chat with friends ! <br> That's Leucos for you</div>
    <img class="send" src="/static/img/send.svg" alt="">
    <div class="container">
        <header>
            <div class="header-logo">
                <img src="/static/img/logo.png" alt="">
            </div>
            <div class="header-text"><b>Leucos</b></div>
        </header>
        <section class="section-container">
            <form action="/chatroom">
                <div class="form-control">
                    <label for="username">Username</label>
                    <input type="text" name="username" id="username" placeholder="       Enter username..." required
                        autocomplete="off">
                </div>
                <div class="form-control">
                    <label for="room">Room</label>
                    <select name="room" id="room" required>
                        <option value="">Select a room</option>
                        <option value="General">General</option>
                        <option value="Games">Games</option>
                        <option value="Movies">Movies</option>
                        <option value="Music">Music</option>
                        <option value="Code">Code</option>
                        <option value="DSA">DSA</option>
                    </select>
                </div>
                <button type="submit" class="btn">Join Chat</button>
            </form>
        </section>
    </div>
